[Desktop Entry]
Name=Parole Media Player
Comment=Play your media
Icon=org.xfce.parole
Exec=parole %F
Terminal=false
Type=Application
Categories=GTK;AudioVideo;Player;Video;
StartupNotify=true
Keywords=Media;Player;Audio;Video;Music;Playback;Xfce;

Actions=Play;Previous;Next;

[Desktop Action Play]
Exec=parole --play
Name=Play/Pause
Icon=media-playback-start-symbolic

[Desktop Action Previous]
Exec=parole --previous
Name=Previous Track
Icon=media-skip-backward-symbolic

[Desktop Action Next]
Exec=parole --next
Name=Next Track
Icon=media-skip-forward-symbolic
